<!-- This is where the user sees all their created decks -->
{{#each model as |deck|}}
  {{#link-to "deck" deck.id}} {{deck-component name=deck.name}} {{/link-to}}
  {{#each deck.flashcards as |flashcard|}}
    {{flashcard.id}}
    {{flashcard.term}}
  {{/each}}
{{/each}}
<br>
{{#link-to 'createdeck' tagName='button'}} Create New Deck{{/link-to}}
